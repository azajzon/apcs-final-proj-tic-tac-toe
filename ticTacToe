public class ticTacToe {
	public static int[][] board;

	public ticTacToe() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		

	}

}




import java.util.Scanner;

public class game {
	private player1 p1 = new player1();
	private player2 p2 = new player2();
	
	
	
	
	public game() {
		//initialize board with no members
		// 0  = "O", 1 = "X", -1 = not yet filled
		for(int i = 0; i<3; i++) {
			for(int j = 0; j<3;j++) {
				ticTacToe.board[i][j] = -1;
			}
			
		}
	}
	
	public void runGame() {
		boolean firstPlayer = true;
		boolean gameEnded = false;
		while(!gameEnded) {
			int[] coords;
			if(firstPlayer) {
				coords = p1.readCoords();
				p1.setCoords(coords[0], coords[1]);
				firstPlayer = false;
				
			}
			else {
				coords = p2.readCoords();
				p2.setCoords(coords[0], coords[1]);
				firstPlayer = true;
			}
		}
			
	}
	
		
		
		
	}
import java.util.Scanner;

/**
 * 
 */

/**
 * @author abzaj
 *
 */
public abstract class player {
	

	/**
	 * 
	 */
	//constructor
	public player() {
		// TODO Auto-generated constructor stub
		
	}
	public int[] readCoords() {
		Scanner scan = new Scanner(System.in);
		int[] coordinates = new int[2];
		boolean cont = true;
		while(cont) {
			System.out.println("Input your X coordinate between 1 & 3. ");
			int x1 = scan.nextInt();
			while(x1 < 1 || x1>3) {
				System.out.println("Number must be between 1 & 3. ");
				x1 = scan.nextInt();
			}
			System.out.println("Input your Y coordinate between 1 & 3");
			int y1 = scan.nextInt();
			while(y1 < 1 || y1>3) {
				System.out.println("Number must be between 1 & 3. ");
				y1 = scan.nextInt();
			}
			
			if(ticTacToe.board[x1][y1] != -1) 
				System.out.print("Position is taken, pick new coordinates");
			else {
				cont = false;
				coordinates[0] = x1;
				coordinates[1] = y1;
			}
		}
		return coordinates;
	}
	public abstract void setCoords(int x, int y);
	public abstract boolean hasWon(int[] coordinates);
}
	


public class player1 extends player {

	public player1() {
		// TODO Auto-generated constructor stub
	}

	public void setCoords(int x, int y) {
		//sets position to "X"
		ticTacToe.board[x][y] = 1;
	}
	public boolean hasWon(int[] coordinates) {
		for(int i = 0; i< 3; i++) {
			if(coordinates[i] )
		}
		return true;
	}
}

public class player2 extends player {

	public player2() {
		// TODO Auto-generated constructor stub
	}

	public void setCoords(int x, int y) {
		//sets position to "O"
		ticTacToe.board[x][y] = 0;
	}
	
}


